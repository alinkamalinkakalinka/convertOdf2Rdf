<?xml version="1.0" encoding="UTF-8"?>
<!-- Example of Object with Sub-Objects. -->
<Objects xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="odf.xsd">
    <Object type="Pedestrian traffic light">
        <id>PTL_123</id>
        <InfoItem id="Red light status">
            <id>RedLight</id>
            <value>on</value>
        </InfoItem>
        <InfoItem id="Green light status">
            <id>GreenLight</id>
            <value>off</value>
        </InfoItem>
        <InfoItem id="Status change frequency">
            <id>StatusChangeFrequency</id>
            <description>Time in seconds the lights switch status</description >
            <value>15</value>
        </InfoItem>
    </Object>
</Objects>
